                        <a class="nav-link uleth_main_nav_link"
                        target="_blank"
                        href=" { { link } } "
                        > { { title } } </a>
                    </li>
                { { / uleth_header_links } }
            </ul>
        </div>
        -->

        <div class="uleth_links" id="uleth_burgers_n_pop">
            <span class="collapse navbar-collapse" id="uleth_burgers_fries_n_pop">
            <ul class="navbar-nav">
                {{#uleth_header_links}}
                    <li class="nav-item">
                        <a class="nav-link uleth_main_nav_link"
                        target="_blank"
                        href="{{link}}"
                        >{{title}}</a>
                    </li>
                {{/uleth_header_links}}
            </ul>
            </span>
        </div>

        <!-- ************************************************************ -->
        <!-- ************************************************************ -->

        <ul class="navbar-nav d-none d-md-flex">
            <!-- custom_menu -->
            {{{ output.custom_menu }}}
            <!-- page_heading_menu -->
            {{{ output.page_heading_menu }}}
        </ul>
        <ul class="nav navbar-nav ml-auto uleth_user_menu">
            <div class="d-none d-lg-block">
                {{{ output.search_box }}}
            </div>
            
            <!-- navbar_plugin_output -->
            <li class="nav-item">
                {{{ output.navbar_plugin_output }}}
            </li>
            
            <!-- user_menu -->
            <li class="nav-item d-flex align-items-center uleth_user_menu_items">
                {{{ output.user_menu }}}
            </li>
        </ul>
        <!-- search_box -->
