<div id="page">

    <div id="page-content">
        <main id="moodle-page" class="clearfix">
            {{{custommenuspacer}}}
            <div id="page-header" class="clearfix">
                <div class="breadcrumb-nav" aria-label="breadcrumb">
                    {{{snapnavbar}}}
                 </div>
                 <div id="page-mast">
                    {{{pageheading}}}
                    {{{courseheader}}}
                    {{{browseallcourses}}}
                </div>
            </div>
            <div id="region-main-box" class="my_courses_page_content">
                <section id="region-main">
                    <div class="row">
                        <div class='{{blockmyoverviewclasses}}'>
                            {{{coursesoptions}}}
                             {{{maincontent}}}
                             {{#enableblockmessage}}
                             <div class="p-3 alert alert-warning">
                                <span>{{#str}}enableblockmyoverviewwarning, theme_snap{{/str}}</span>
                             </div>
                             {{/enableblockmessage}}
                         </div>
                         <div class="snap_feeds_columncol col-sm-12 col-xl-3">
                             {{{snapfeeds}}}
                         </div>
                     </div>
                 </section>
             </div>
             {{{standaraftermainregion}}}
             {{{snapblocks}}}
        </main>
    </div>
</div>
{{#js}}
    require(['theme_snap/my_courses'], function(my_courses) {
    my_courses.init({{sitepolicyacceptreqdmycourses}});
    });
{{/js}}
